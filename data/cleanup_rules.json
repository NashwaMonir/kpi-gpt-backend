[
  {
    "id": "remove_double_the",
    "pattern": "\\bthe\\s+the\\b",
    "flags": "gi",
    "replace": "the"
  },
  {
    "id": "remove_double_and",
    "pattern": "\\b(and)\\s+and\\b",
    "flags": "gi",
    "replacement": "$1"
  },
  {
    "id": "remove_double_to",
    "pattern": "\\b(to)\\s+to\\b",
    "flags": "gi",
    "replacement": "$1"
  },
  {
    "id": "collapse_multiple_commas",
    "pattern": ",{2,}",
    "flags": "g",
    "replacement": ","
  },
  {
    "id": "trim_comma_before_and",
    "pattern": ",\\s+and\\s*,",
    "flags": "gi",
    "replacement": ", and "
  },
  {
    "id": "remove_trailing_comma_before_end",
    "pattern": ",\\s*$",
    "flags": "g",
    "replacement": ""
  },
  {
    "id": "fix_comma_before_clause",
    "pattern": ",\\s+(and|while|in support of|aligned with)",
    "flags": "gi",
    "replacement": ", $1"
  },
  {
    "id": "normalize_achieving_with",
    "pattern": "achieving\\s+with\\s+",
    "flags": "gi",
    "replacement": "achieving "
  },
  {
    "id": "fix_achieving_ensure",
    "pattern": "\\bachieving\\s+ensure\\b",
    "flags": "gi",
    "replacement": "ensuring"
  },
  {
  "id": "fix_to_better_gerund",
  "pattern": "\\bto\\s+better\\s+(improving|increasing|enhancing|optimizing|streamlining|strengthening|reducing|lowering|maximizing|minimizing)\\b",
  "flags": "gi",
  "replace": "to $1"
  },
  {
    "id": "fix_with_a_focus_on_to",
    "pattern": "\\bwith\\s+a\\s+focus\\s+on\\s+to\\b",
    "flags": "gi",
    "replacement": "with a focus on"
  },
  {
    "id": "remove_and_also",
    "pattern": "\\band\\s+also\\b",
    "flags": "gi",
    "replacement": "and"
  },
  {
    "id": "remove_extra_with_with",
    "pattern": "\\bwith\\s+with\\b",
    "flags": "gi",
    "replacement": "with "
  },
  {
    "id": "fix_strengthen_supporting",
    "pattern": "\\bstrengthen\\s+supporting\\b",
    "flags": "gi",
    "replacement": "supporting"
  },
  {
    "id": "fix_strengthen_support",
    "pattern": "\\bstrengthen\\s+support\\b",
    "flags": "gi",
    "replacement": "support"
  },
  {
    "id": "dedupe_supporting_enhancing",
    "pattern": "\\bsupporting\\s+([^,.]{0,120}?)\\s+and\\s+enhancing\\s+\\1\\b",
    "flags": "gi",
    "replacement": "supporting $1"
  },
  {
    "id": "dedupe_supporting_and_enhancing",
    "pattern": "\\bsupporting\\s+([^,.]{0,120}?)\\s+and\\s+enhancing\\s+",
    "flags": "gi",
    "replacement": "supporting $1, "
  },
  {
    "id": "dedupe_aligned_with_aligned_with",
    "pattern": "\\baligned\\s+with\\s+([^,.]{0,120}?)\\s+aligned\\s+with\\b",
    "flags": "gi",
    "replacement": "aligned with $1"
  },
  {
    "id": "collapse_double_while",
    "pattern": "\\bwhile\\b\\s+([^,.]{0,160}?)\\s+\\bwhile\\b",
    "flags": "gi",
    "replacement": "while $1 and"
  }
]