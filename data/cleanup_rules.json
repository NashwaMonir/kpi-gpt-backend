[
  {
    "id": "remove_double_and",
    "pattern": "\\b(and)\\s+and\\b",
    "flags": "gi",
    "replacement": "$1"
  },
  {
    "id": "remove_double_to",
    "pattern": "\\b(to)\\s+to\\b",
    "flags": "gi",
    "replacement": "$1"
  },
  {
    "id": "collapse_multiple_commas",
    "pattern": ",{2,}",
    "flags": "g",
    "replacement": ","
  },
  {
    "id": "trim_comma_before_and",
    "pattern": ",\\s+and\\s*,",
    "flags": "gi",
    "replacement": ", and "
  },
  {
    "id": "remove_trailing_comma_before_end",
    "pattern": ",\\s*$",
    "flags": "g",
    "replacement": ""
  },
  {
    "id": "fix_comma_before_clause",
    "pattern": ",\\s+(and|while|in support of|aligned with)",
    "flags": "gi",
    "replacement": ", $1"
  },
  {
    "id": "normalize_achieving_with",
    "pattern": "achieving\\s+with\\s+",
    "flags": "gi",
    "replacement": "achieving "
  },
  {
    "id": "remove_extra_with_with",
    "pattern": "\\bwith\\s+with\\b",
    "flags": "gi",
    "replacement": "with "
  }
]